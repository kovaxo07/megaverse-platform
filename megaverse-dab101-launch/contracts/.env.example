# Deployment Configuration
# Copy this file to .env and fill in your values

# RPC URLs
RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
RPC_URL_TESTNET=https://polygon-mumbai.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Deployer private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# PolygonScan API key for contract verification
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# Example values (DO NOT USE IN PRODUCTION):
# RPC_URL=https://polygon-rpc.com
# PRIVATE_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef
# POLYGONSCAN_API_KEY=ABC123DEF456GHI789JKL012MNO345PQR678STU